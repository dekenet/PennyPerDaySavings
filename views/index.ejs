<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script>
      function loadContent(c) {
        content = document.getElementById('content').firstElementChild;
        content.setAttribute('src', '/content?c='+c)
      }
    </script>
  </head>
  <body>
    <header id="header">
      <%- include('../views/header.ejs', {title: title}) %>
    </header>
    <nav id="nav">
      <%- include('../views/nav.ejs', {nav: nav, s: c}) %>
    </nav>
    <section id="content">
      <iframe scrolling="no" src="/content?c=<%=c%>"></iframe>
    </section>
    <footer id="footer"></footer>
  </body>
</html>
